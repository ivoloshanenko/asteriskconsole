<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>AsterixConsole</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Le styles -->
    <link href="assets/libs/bootstrap/css/bootstrap.css" rel="stylesheet">
    <link href="assets/libs/bootstrap/css/bootstrap-responsive.css" rel="stylesheet">

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="assets/js/html5shiv.js"></script>
    <![endif]-->

  </head>

  <body>

    <section class="container" style="margin-top:20px;">

      <header>
        <div class="navbar">
          <div class="navbar-inner">
            <a class="brand" href="/">AsterixConsole</a>
            <ul class="nav">
              <li class="active"><a href="/">Users</a></li>
              <li><a href="#">..</a></li>
            </ul>
          </div>
        </div>
      </header>

      <section class="content">

        <div class="row-fluid">

          <!-- SEARCH -->
          <form class="form-search span4">
            <fieldset>
              <div class="input-append">
                <input type="text" class="span12 search-query">
                <button type="submit" class="btn">Search</button>
              </div>
            </fieldset>
          </form>

          <!-- CREATE -->
          <div class="span6">
            <a href="#create" role="button" class="btn btn-medium btn-success" data-toggle="modal"><i class="icon-plus icon-white"></i> Create</a>
          </div>

        </div>

        <!-- CONTENT -->
        <table class="table table-striped table-hover">
          <thead>
            <tr>
              <th>Name</th>
              <th>Secret</th>
              <th>Callerid</th>
              <th>Context</th>
              <th>Pickupgroup</th>
              <th>Permit</th>
              <th>Nat</th>
              <th>IPaddr</th>
              <th>Useragent</th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>name</td>
              <td>secret</td>
              <td>callerid</td>
              <td>context</td>
              <td>pickupgroup</td>
              <td>permit</td>
              <td>nat</td>
              <td>ipaddr</td>
              <td>useragent</td>
              <td>
                <button class="btn"><i class="icon-edit"></i></button>
                <button class="btn btn-danger"><i class="icon-remove icon-white"></i></button>
              </td>
            </tr>
            <tr>
              <td>name</td>
              <td>secret</td>
              <td>callerid</td>
              <td>context</td>
              <td>pickupgroup</td>
              <td>permit</td>
              <td>nat</td>
              <td>ipaddr</td>
              <td>useragent</td>
              <td>
                <button class="btn"><i class="icon-edit"></i></button>
                <button class="btn btn-danger"><i class="icon-remove icon-white"></i></button>
              </td>
            </tr>
            <tr>
              <td>name</td>
              <td>secret</td>
              <td>callerid</td>
              <td>context</td>
              <td>pickupgroup</td>
              <td>permit</td>
              <td>nat</td>
              <td>ipaddr</td>
              <td>useragent</td>
              <td>
                <button class="btn"><i class="icon-edit"></i></button>
                <button class="btn btn-danger"><i class="icon-remove icon-white"></i></button>
              </td>
            </tr>
          </tbody>
        </table>

      </section>

    </section>

    <!-- CREATE FORM -->
    <div id="create" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="createLabel" aria-hidden="true" method="post">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
        <h3 id="createLabel">Create</h3>
      </div>
      <form action="/create" id="createForm" class="modal-body form-horizontal">
        <div class="control-group">
          <label class="control-label" for="createFormName">Name</label>
          <div class="controls">
            <input type="text" id="createFormName" name="user[name]" placeholder="Name">
          </div>
        </div>
        <div class="control-group">
          <label class="control-label" for="createFormSecret">Secret</label>
          <div class="controls">
            <input type="text" id="createFormSecret" name="user[secret]" placeholder="Secret">
          </div>
        </div>
        <div class="control-group">
          <label class="control-label" for="createFormCallerid">Callerid</label>
          <div class="controls">
            <input type="text" id="createFormCallerid" name="user[callerid]" placeholder="Callerid">
          </div>
        </div>
        <div class="control-group">
          <label class="control-label" for="createFormContext">Context</label>
          <div class="controls">
            <input type="text" id="createFormContext" name="user[context]" placeholder="Context">
          </div>
        </div>
        <div class="control-group">
          <label class="control-label" for="createFormPickupgroup">Pickupgroup</label>
          <div class="controls">
            <input type="text" id="createFormPickupgroup" name="user[pickupgroup]" placeholder="Pickupgroup">
          </div>
        </div>
        <div class="control-group">
          <label class="control-label" for="createFormCallgroup">Callgroup</label>
          <div class="controls">
            <input type="text" id="createFormCallgroup" name="user[callgroup]" placeholder="Callgroup">
          </div>
        </div>
        <div class="control-group">
          <label class="control-label" for="createFormNat">Nat</label>
          <div class="controls" name="user[nat]">
            <select id="createFormNat">
              <option value="1">Yes</option>
              <option value="0">No</option>
            </select>
          </div>
        </div>
        <div class="control-group">
          <label class="control-label" for="createFormPermit">Permit</label>
          <div class="controls">
            <input type="text" id="createFormPermit" placeholder="Permit">
          </div>
        </div>
      </form>
      <div class="modal-footer">
        <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
        <button type="submit" class="btn btn-success" form="createForm">Create</button>
      </div>
    </div>

    <!-- Le javascript
