<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>AsterixConsole</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Le styles -->
    <link href="assets/libs/bootstrap/css/bootstrap.css" rel="stylesheet">
    <link href="assets/libs/bootstrap/css/bootstrap-responsive.css" rel="stylesheet">

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="assets/js/html5shiv.js"></script>
    <![endif]-->

  </head>

  <body>

    <section class="container" style="margin-top:20px;">

      <header>
        <div class="navbar">
          <div class="navbar-inner">
            <a class="brand" href="/">AsterixConsole</a>
            <ul class="nav">
              <li class="active"><a href="/">Users</a></li>
              <li><a href="#">..</a></li>
            </ul>
          </div>
        </div>
      </header>

      <section class="content">

        <div class="row-fluid">

          <!-- SEARCH -->
          <form class="form-search span4">
            <fieldset>
              <div class="input-append">
                <input type="text" class="span12 search-query">
                <button type="submit" class="btn">Search</button>
              </div>
            </fieldset>
          </form>

          <!-- CREATE -->
          <div class="span6">
            <a href="#create" role="button" class="btn btn-medium btn-success" data-toggle="modal"><i class="icon-plus icon-white"></i> Create</a>
          </div>

        </div>

        <!-- CONTENT -->
        <table class="table table-striped table-hover">
          <thead>
            <tr>
              <th>Name</th>
              <th>Secret</th>
              <th>Callerid</th>
              <th>Context</th>
              <th>Pickupgroup</th>
              <th>Permit</th>
              <th>Nat</th>
              <th>IPaddr</th>
              <th>Useragent</th>
              <th></th>
            </tr>
          </thead>
          <tbody id="users-list"></tbody>
        </table>

      </section>

    </section>

    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="assets/libs/jquery/jquery.js"></script>
    <script src="assets/libs/bootstrap/js/bootstrap.min.js"></script>
    <script src="assets/libs/jquery.maskedinput/jquery.maskedinput.js"></script>
    <script src="assets/libs/jquery.scrollto/jquery.scrollto.js"></script>
    <script src="assets/libs/underscore/underscore.js"></script>
    <script src="assets/libs/twig/twig.js"></script>
    <script src="assets/app.js"></script>

  </body>
</html>
